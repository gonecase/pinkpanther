    </div>
</form>
</div>

<!--End mc_embed_signup-->

		<div id="article-inline" class="modal-article-inline">
			<div id="article-inline-container" class="modal-article-inline-container">
				adlkmlkadmlsadmlsml
			</div>
		</div>

		{% block footer %}
			<footer id="footer" style="z-index: 10010;position: relative;">
				{% include 'footer.twig' %}
			</footer>
			{{ function('wp_footer') }}
		{% endblock %}
	</body>
</html>
