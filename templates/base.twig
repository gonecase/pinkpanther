{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body class="{{body_class}} has-navbar-fixed-top" data-template="base.twig">
		{% block preheader %}

		{% endblock %}
		<header id="header" class="header" >

			<nav class="navbar" role="navigation" aria-label="main navigation">
				<div class="navbar-brand">
					<!-- navbar items, navbar burger... -->
					<a class="navbar-item" href="{{site.url}}" rel="home" STYLE="width: 110%;">
						<img src="{{theme.theme_mod('pink_panther_logo')}}" alt="Deadant">
					</a>
					<a role="button" class="navbar-burger" aria-label="menu" aria-expanded="false">
						<span aria-hidden="true"></span>
						<span aria-hidden="true"></span>
						<span aria-hidden="true"></span>
					</a>
				</div>
				<div class="navbar-menu" id="navMenu">
					<div class="navbar-end">
						{% include "menu.twig" with {'menu': menu.get_items} %}
					</div>
				</div>
        <div class="searchform-nav">
					{{function('get_search_form')}}
					<div class="search-toggle search-close"> ✕ </div>
				</div>
				<div class="search-main search-toggle">
					<svg width="22px" height="22px" viewBox="0 0 32 32">
						<g class="js-transition-icon__state">
							<line fill="none" stroke="#111111" x1="30" y1="30" x2="21.5" y2="21.5"/>
							<circle fill="none" stroke="#111111" cx="13" cy="13" r="12"/>
						</g>
					</svg>
				</div>
			</nav>
			{% block header %}

			{% endblock %}
		</header>
		{# <div class="advertisement-top advertisement-holder">
				<div class="advertisement">
					<img src="https://via.placeholder.com/970x250.png?text=advertisement:970x250" alt="">
				</div>
          </div> #}
		<div class="main main-content">
			<section id="content" role="main" class="container {{post.post_type}}-wrapper">
				<div class="row">
					<div class="wrapper col col-12 col-xs-12">
						{% if not is_home %}
							{% if not is_single %}
								{% block title %}
                  {% if wp_title == 'Listings' %}
                    <h1 class="not-home-main-title">{{listing_title}}</h1>
                    <br>
                    <br>
                    <h3>Standup comedy is best experienced live. <br />
										GTFO and go watch your favourite comedian in the flesh.</h3>
										<br/>
										<p style="max-width: 500px;">(This listing of events is as accurate as we can make it, but comedians are unpredictable people and everything is subject to change. Change being the constant and all…)</p>
                    <br>
                    <br>
                  {% else %}
                    <h1 class="not-home-main-title">All articles in : {{wp_title}}</h1>
                  {% endif %}
                {% endblock %}
							{% endif %}
						{% endif %}
						{% block fullsize %}{% endblock %}
					</div>
				</div>
				<div class="row">
					<div class="{% if sidebar %}content-wrapper{% endif %} wrapper">
						<div id="posts-listing-container" >
							{% block content %}
							{% endblock %}
						</div>
					</div>
					{% if sidebar %}
						<aside class="layout-sidebar sidebar">
              <div id="listing-search">
								{% include "partial/listing-sidebar.twig" %}
							</div>
							{# {% if sticky_event.count > 0 and close_events.count > 0 %}
                <h2 class="rounded">Events</h2>
								{% include "partial/events-sidebar.twig" with {sticky_event: sticky_event, close_events: close_events} %}
							{% endif %} #}
							<div id="sidebar-holder" class="sidebar-holder">
								{{sidebar}}
							</div>
						</aside>
					{% endif %}
					{% block postsidebar %}
					{% endblock %}
				</div>
				<div class="row">
					<div class="content-wrapper">
						{% block pagination %}
						{% endblock %}
					</div>
					<aside class="layout-sidebar">
					</aside>
				</div>
			</section>
		</div>

    {% include "partial/loader.twig" %}

    {# <script type="text/javascript" src="//downloads.mailchimp.com/js/signup-forms/popup/unique-methods/embed.js" data-dojo-config="usePlainJson: true, isDebug: false"></script><script type="text/javascript">window.dojoRequire(["mojo/signup-forms/Loader"], function(L) { L.start({"baseUrl":"mc.us20.list-manage.com","uuid":"6de5374a88579deb0f1de255d","lid":"c688a6de3d","uniqueMethods":true}) })</script> #}
